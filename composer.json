{
    "name": "cyclear/cyclear",
    "description": "A cycling-game in the Cyclear tradition",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.11",
        "friendsofsymfony/user-bundle": "^4",
        "knplabs/knp-paginator-bundle": "^6",
        "stof/doctrine-extensions-bundle": "*",
        "symfony/acl-bundle": "^2.3",
        "symfony/asset": "^7",
        "symfony/cache": "^7",
        "symfony/config": "^7",
        "symfony/console": "^7",
        "symfony/css-selector": "^7",
        "symfony/dependency-injection": "^7",
        "symfony/doctrine-bridge": "^7",
        "symfony/dom-crawler": "^7",
        "symfony/dotenv": "^7",
        "symfony/error-handler": "^7",
        "symfony/event-dispatcher": "^7",
        "symfony/flex": "^2.2",
        "symfony/form": "^7",
        "symfony/filesystem": "^7",
        "symfony/finder": "^7",
        "symfony/framework-bundle": "^7",
        "symfony/google-mailer": "^7",
        "symfony/http-client": "^6",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^7",
        "symfony/intl": "^7",
        "symfony/lock": "^7",
        "symfony/mailer": "^7",
        "symfony/mime": "^7",
        "symfony/monolog-bundle": "^3",
        "symfony/monolog-bridge": "^7",
        "symfony/options-resolver": "^7",
        "symfony/password-hasher": "^7",
        "symfony/process": "^7",
        "symfony/property-access": "^7",
        "symfony/property-info": "^7",
        "symfony/routing": "^7",
        "symfony/runtime": "^7",
        "symfony/security-bundle": "^7",
        "symfony/security-csrf": "^7",
        "symfony/security-core": "^7",
        "symfony/security-http": "^7",
        "symfony/serializer": "^7",
        "symfony/string": "^7",
        "symfony/templating": "^6",
        "symfony/translation": "^7",
        "symfony/twig-bundle": "^7",
        "symfony/twig-bridge": "^7",
        "symfony/validator": "^7",
        "symfony/yaml": "^7",
        "twig/extra-bundle": "^3.4",
        "twig/intl-extra": "^3.4",
        "twig/twig": "^3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3",
        "liip/functional-test-bundle": "^4.5",
        "liip/test-fixtures-bundle": "^2.4",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.2.8",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^7",
        "symfony/debug-bundle": "^7",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "^7",
        "symfony/var-dumper": "^7",
        "symfony/web-profiler-bundle": "^7",
        "vimeo/psalm": "^5",
        "vincentlanglet/twig-cs-fixer": "^3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "lint-twig": "php bin/console lint:twig --show-deprecations templates/ --ansi && vendor/bin/twig-cs-fixer lint templates/",
        "lint-twig-fix": "vendor/bin/twig-cs-fixer lint --fix templates/",
        "lint-yaml": "php bin/console lint:yaml translations/ config/ --parse-tags",
        "psalm": "psalm --no-cache",
        "test": "APP_ENV=test vendor/bin/phpunit -c . tests/"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*",
            "docker": false
        }
    }
}
